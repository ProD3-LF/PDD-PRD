if test "$PD3_PREFIX" == ""
then
	echo "PD3_PREFIX not set, using parent dir"
else
PD3_PREFIX="$PD3_PREFIX/"
fi
SLDMODELDIR=`./getSldParameter SLDMODELDIR`
rm -rf $SLDMODELDIR.*
rm -rf $SLDMODELDIR
mkdir -p $SLDMODELDIR
rm -rf ../central/observations/collected/*/SLD
rm -rf ../central/observations/uncollected/*/SLD
echo "TIME03: 1000" > $SLDMODELDIR/SLDMODEL
echo "TIME97: 1" >> $SLDMODELDIR/SLDMODEL
echo "TIME0: 1000" >> $SLDMODELDIR/SLDMODEL
echo "TIME100: 1" >> $SLDMODELDIR/SLDMODEL
echo "COUNT03: 1000" >> $SLDMODELDIR/SLDMODEL
echo "COUNT97: 1" >> $SLDMODELDIR/SLDMODEL
echo "COUNT0: 1000" >> $SLDMODELDIR/SLDMODEL
echo "COUNT100: 1" >> $SLDMODELDIR/SLDMODEL
touch  -d 01/01/1970 $SLDMODELDIR/SLDMODEL
